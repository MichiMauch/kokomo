# Cloudflare R2 Configuration
NEXT_PUBLIC_CLOUDFLARE_R2_URL=https://your-r2-url.r2.dev
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_ACCESS_KEY_ID=your_access_key
CLOUDFLARE_SECRET_ACCESS_KEY=your_secret_key
CLOUDFLARE_BUCKET_2=your_bucket_name

# GitHub Integration
GITHUB_TOKEN=your_github_token
GITHUB_REPO_OWNER=your_username
GITHUB_REPO_NAME=your_repo_name
GITHUB_BRANCH=main
GITHUB_PATH=data/tiny-house

# Admin Credentials
ADMIN_USER=admin
ADMIN_PASSWORD=secure_password

# Matomo Analytics
MATOMO_TOKEN=your_matomo_token
MATOMO_TOKEN_NETNODE=your_netnode_token

# OpenAI API (required for AI features)
OPENAI_API_KEY=sk-proj-your_openai_api_key

# AgentKit Configuration (optional)
# Set to false to disable AgentKit draft functionality
# AGENTKIT_ENABLED=true
